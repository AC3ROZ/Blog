<h1>Editing Entry</h1>


<%= form_for(@entry) do |f| %>
  <%= f.label :title,"title" %>
  <%= f.text_field :title %>

  <%= f.label :tag_list, "tag" %>
  <%= text_field_tag 'entry[tag_list]', @entry.tag_list.join(',') %>

  <%= f.label :body_md,"body" %>
  <%= f.text_area :body_md %>
  <%= f.submit "投稿" %>
<% end %>
<%= link_to 'Show', @entry %> |
<%= link_to 'Back', entries_path %>
