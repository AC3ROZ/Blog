<h1>Sessions#new</h1>
<% if logged_in? %>
  <p>ぽえぽえ</p>
<% end %>
<%= form_for(:session, url:login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "login" %>
<% end %>
<p>Find me in app/views/sessions/new.html.erb</p>
